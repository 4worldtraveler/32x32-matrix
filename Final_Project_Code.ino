// We're making these globals to save stack space.
int row;                        ///< Row count, when looping.
int col;                        ///< Column count, when looping.
unsigned int red;              ///< Red color value, pulled from the palette.
unsigned int green;            ///< Green color value, pulled from the palette.
unsigned int blue;             ///< Blue color value, pulled from the palette.
unsigned int pixelPosition;    ///< Index into the pixel array.
uint16_t mixedColor;            ///< 24-bit RGB color, converted to an RGB Matrix color.
unsigned int cycle;             ///< Animation cycle counter.

void setup() 
{
  // Begins Matrix
    matrix.begin();
  // Makes 32x32 Matrix Blank
    matrix.fillScreen(matrix.Color888(0, 0, 0));
}

// Function to Draw Megaman
void draw_megaman(const byte *Pixal_Array)
{
    pixelPosition = 0;  // Sets Pixel Positon to 0
  
    for (row = 0; row < 32; row++) //Start Row loop for 32x32 Matrix
    {
        for (col = 0; col < 32; col++) //Start Column loop for 32x32 Matrix
        {
            
            red = 0;  //Set Red to 0 for each loop
            green = 0; //Set Green to 0 for each loop
            blue = 0; //Set Blue to 0 for each loop
            
            //read 
            red = pgm_read_byte(&Pixal_Array[pixelPosition]);   // Reads the element of the array that is stored in PROGMEM and sets it to Red.
            green = pgm_read_byte(&Pixal_Array[pixelPosition+1]); // Reads the element of the array that is stored in PROGMEM and sets it to Green.
            blue = pgm_read_byte(&Pixal_Array[pixelPosition+2]); // Reads the element of the array that is stored in PROGMEM and sets it to Blue.
            
            mixedColor = matrix.Color888(red, green, blue); //converts RGB Values to The color that the RGB Values Point to.
            matrix.drawPixel(col, row, mixedColor);  // Lights up the LED on the Column and Row that its on with the Color The "mixedColor" Provided.
            pixelPosition = pixelPosition + 3; //Adds 3 to the Pixel Position Because each 3 values in the Array is one Color.
        }
    }
}
 
 void megaman(unsigned int cycles)
{
    matrix.fillScreen(matrix.Color888(0, 0, 0));
    for (cycle = 0; cycle < cycles; cycle++)
    {
        draw_megaman(MEGAMAN_FRAME_1_RGB); //calls 1st Mega Man Frame
        delay(500);
        draw_megaman(MEGAMAN_FRAME_2_RGB); //calls 2nd Mega Man Frame
        delay(500);
        draw_megaman(MEGAMAN_FRAME_3_RGB); //calls 3rd Mega Man Frame
        delay(500);
        draw_megaman(MEGAMAN_FRAME_4_RGB); //calls 4th Mega Man Frame
        delay(500);
    }
}



void loop() 
{    
  megaman(1); // Starts Mega man Animation
}
